{
    "extends": "standard",
    "root": true,
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [ "html", "standard", "vue" ],
    "rules": {
        "indent": [0, 2, { "SwitchCase": 1 }],
        "quotes": ["error", "single"],
        "semi": [0],
        "no-console": [0],
        "no-empty": 2,
        "no-eq-null": 2,
        "no-new": 0,
        "no-fallthrough": 0,
        "no-unreachable": 0,
        "camelcase": [0,{"properties": "never"}],
        "one-var": 0
    },
    "globals": {
        "BASE_URL": true,
        "CLIENT_ID": true
    }
}
